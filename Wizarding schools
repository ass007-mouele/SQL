UPDATE school SET country = 'Sweden' WHERE name  = 'Durmstrang Institute';
UPDATE school SET capacity = '700' WHERE name  = 'Mahoutokoro School of Magic';
DELETE FROM school where name LIKE '%Magic%';
SELECT * FROM school;


